    }, backOut                  : function (d)
    {
        d = d - 1;
        var b = 1.70158;
        return d * d * ((b + 1) * d + b) + 1
    }, elastic                  : function (i)
    {
        if(i == 0 || i == 1)
        {
            return i
        }
