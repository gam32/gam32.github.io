function $try()
{
    for(var b = 0, a = arguments.length; b < a; b++)
    {
        try
        {
            return arguments[b]()
        }
        catch (c)
        {
        }
    }
    return null
}
function $type(a)
{
    if(a == undefined)
    {
        return false
    }
    if(a.$family)
    {
        return(a.$family.name == "number" && !isFinite(a)) ? false : a.$family.name
    }
    if(a.nodeName)
    {
        switch (a.nodeType)
        {
            case 1:
                return"element";
            case 3:
                return(/\S/).test(a.nodeValue) ? "textnode" : "whitespace"
        }
    } else
    {
        if(typeof a.length == "number")
        {
            if(a.callee)
            {
                return"arguments"
            } else
            {
                if(a.item)
                {
                    return"collection"
                }
            }
        }
    }
    return typeof a
}
function $unlink(c)
{
    var b;
    switch ($type(c))
    {
        case"object":
            b = {};
            for(var f in c)
            {
                b[f] = $unlink(c[f])
            }
            break;
        case"hash":
            b = new Hash(c);
            break;
        case"array":
            b = [];
            for(var d = 0, a = c.length; d < a; d++)
            {
                b[d] = $unlink(c[d])
            }
            break;
        default:
            return c
    }
    return b
}
