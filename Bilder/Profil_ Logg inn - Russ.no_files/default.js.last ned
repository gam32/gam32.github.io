    for(var A = 0, w = c.length; A < w; A++)
    {
        var z = c[A];
        if(A == 0 && Selectors.RegExps.quick.test(z))
        {
            q = p.getElementsByTagName(z);
            continue
        }
        var a = b[A - 1];
        var r = Selectors.Utils.parseTagAndID(z);
        var C = r[0], s = r[1];
        if(A == 0)
        {
            q = Selectors.Utils.getByTagAndID(p, C, s)
        } else
        {
            var d = {}, h = [];
            for(var y = 0, x = q.length; y < x; y++)
            {
