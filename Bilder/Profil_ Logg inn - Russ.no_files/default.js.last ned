    aI.easing_formulas = {linear: function (b)
    {
        return b
    }, "<"                      : function (b)
    {
        return bl(b, 3)
    }, ">"                      : function (b)
    {
        return bl(b - 1, 3) + 1
    }, "<>"                     : function (b)
    {
        b = b * 2;
