    var b = this.retrieve("move");
    if(b)
    {
        b.cancel()
    }
    return this.eliminate("move").store("move:options", $extend({link: "cancel"}, a))
}, get                        : function (a)
{
    if(a || !this.retrieve("move"))
    {
        if(a || !this.retrieve("move:options"))
        {
            this.set("move", a)
